{
  "name": "image-preloader",
  "public": true,
  "version": "2.0.0-beta",
  "description": "Simple, fast and lightweight html-images preloader",
  "type": "module",
  "source": "src/index.ts",
  "unpkg": "dist/image-preloader.min.js",
  "module": "dist/image-preloader.module.js",
  "main": "dist/image-preloader.cjs",
  "types": "typedefs/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "microbundle --name=ImagePreloader",
    "build:watch": "microbundle --name=ImagePreloader watch",
    "lint": "prettier --write .",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smelukov/ImagePreloader.git"
  },
  "keywords": [
    "image",
    "preloader",
    "html"
  ],
  "author": "Sergey Melyukov",
  "license": "MIT",
  "contributors": [
    "Lucian Last <li@last.nl>"
  ],
  "bugs": {
    "url": "https://github.com/smelukov/ImagePreloader/issues"
  },
  "files": [
    "src",
    "dist/image-preloader.min.js",
    "dist/image-preloader.module.js",
    "dist/image-preloader.cjs",
    "typedefs"
  ],
  "homepage": "https://github.com/smelukov/ImagePreloader#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "canvas": "^2.8.0",
    "jest": "^27.1.1",
    "jest-extended": "^0.11.5",
    "microbundle": "^0.13.3",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  }
}
